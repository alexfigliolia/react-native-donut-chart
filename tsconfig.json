{
	"ts-node": {
		"compilerOptions": {
			"module": "CommonJS"
		}
	},
	"compilerOptions": {
		"target": "ES2015",
		"lib": [
			"dom",
			"dom.iterable",
			"esnext"
		],
		"jsx": "react-native",
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noFallthroughCasesInSwitch": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true
	},
	"include": [
		"ci",
		"package.json",
		".eslintrc.js",
		"src"
	]
}